<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:PrinterManagerProject.Styles">

    <Style x:Key="controlLabel" TargetType="{x:Type Label}">
        <Setter Property="FontSize" Value="20" />
        <Setter Property="Height" Value="34" />
        <Setter Property="Margin" Value="10,5" />
    </Style>
    <Style x:Key="controlTextLabel" TargetType="{x:Type Label}">
        <Setter Property="FontSize" Value="20" />
        <Setter Property="Height" Value="32" />
    </Style>
    <Style x:Key="controlEllipse" TargetType="{x:Type Ellipse}">
        <Setter Property="Height" Value="32" />
        <Setter Property="Width" Value="32" />
    </Style>
</ResourceDictionary>